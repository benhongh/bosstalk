{
  "name": "bosstalk",
  "version": "1.0.0",
  "description": "Source code for bosstalk.net website",
  "private": true,
  "main": "js/server/app.js",
  "scripts": {
    "start": "node lib/server/app.js",
    "clean": "rm -r lib/ && rm -r public/js/ && rm -r public/css/",
