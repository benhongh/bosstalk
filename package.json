    "test": "npm run build-server && mocha lib/tests"
  },
  "keywords": [
    "bosstalk"
  ],
  "author": "Navpreet Bajwa / Hongbin Hu",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.14.2",
    "bootstrap-sass": "^3.3.6",
    "express": "^4.13.3",
    "jquery": "^2.1.4",
    "mongoose": "^4.3.4",
    "node-sass-middleware": "^0.9.7",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "rx": "^4.0.7"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "fs-extra": "^0.26.3",
    "jake": "^8.0.12",
    "mocha": "^2.3.4",
    "ts-loader": "^0.7.2",
    "tsd": "^0.6.5",
    "tslint": "^3.2.1",
    "typescript": "^1.7.3",
    "webpack": "^1.12.9"
  }
